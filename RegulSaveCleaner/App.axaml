<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="RegulSaveCleaner.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://RegulSaveCleaner.Assets/Icons.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
        <ResourceDictionary x:Key="LanguageDictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://RegulSaveCleaner.Assets/Localization/en.axaml" />
                <ResourceInclude Source="avares://RegulSaveCleaner.Assets/Localization/ru.axaml" />
                <ResourceInclude Source="avares://RegulSaveCleaner.Assets/Localization/fr.axaml" />
                <ResourceInclude Source="avares://RegulSaveCleaner.Assets/Localization/zh.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
