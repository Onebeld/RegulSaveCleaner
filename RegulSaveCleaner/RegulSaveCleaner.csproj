<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFrameworks>net6.0;net461</TargetFrameworks>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <ApplicationIcon>icon.ico</ApplicationIcon>
        <IncludeAvaloniaGenerators>true</IncludeAvaloniaGenerators>
        <AssemblyVersion>3.1.0</AssemblyVersion>
        <FileVersion>3.1.0</FileVersion>
        <LangVersion>preview</LangVersion>
        <Product>Regul Save Cleaner</Product>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Avalonia" Version="11.0.0-rc1.1" />
        <PackageReference Include="Avalonia.Desktop" Version="11.0.0-rc1.1" />
        <PackageReference Include="Pfim" Version="0.11.2" />
        <PackageReference Include="PleasantUI" Version="3.0.1" />
        
    </ItemGroup>

    <ItemGroup>
        <AdditionalFiles Include="**\*.axaml" />
        <Compile Update="**\*.axaml.cs" />
        <Compile Include="obj\Debug\net461\.NETFramework,Version=v4.6.1.AssemblyAttributes.cs" />
        <Compile Include="obj\Debug\net6.0\.NETCoreApp,Version=v6.0.AssemblyAttributes.cs" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Views\InformationPages\" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Regul.S3PI\Regul.S3PI.csproj" />
